{
  "configuration": {
    "config": {
      "configName": "config",
      "topScope": [
        "const path = require('path');",
        "const webpack = require('webpack');",
        "\n"
      ],
      "webpackOptions": {
        "mode": "'development'",
        "plugins": [
          "new webpack.ProgressPlugin()"
        ],
        "module": {
          "rules": [
            {
              "test": "/\\.(js|jsx)$/",
              "include": [
                "path.resolve(__dirname, 'src')"
              ],
              "loader": "'babel-loader'"
            }
          ]
        }
      }
    }
  },
